{
  "expo": {
    "name": "BABY-WATCH-ME-GROW-IOS",
    "slug": "BABY-WATCH-ME-GROW-IOS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "BABY-WATCH-ME-GROW-IOS",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to the photo library in order to save the resulting video."
      },
      "bundleIdentifier": "com.xuanduy2309.BABYWATCHMEGROWIOS"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.xuanduy2309.BABYWATCHMEGROWIOS"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-5372862349743986~1080405773",
      "ios_app_id": "ca-app-pub-5372862349743986~3844057102",
      "user_tracking_usage_description": "This identifier will be used to deliver personalized ads to you."
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5372862349743986~1080405773",
          "iosAppId": "ca-app-pub-5372862349743986~3844057102"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a9b09c17-d66b-4e26-9f81-dd0857b7114e"
      }
    }
  }
}
